dumb_rec(a : int) : int {
    b : int;
    b = 4;
    return dumb_rec(b - a) + dumb_rec(b + a) * dumb_rec(a * b );
}

main() {
	i : int;
    i = 42;

    while( i >= 7 ) {
        @i;
        dumb_rec(i + 7 * 13 + 8 );
        i = i + 1;
    }
    return main();
}
